{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "shop-ui": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/shop-ui",
      "sourceRoot": "projects/shop-ui/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/shop-ui",
            "index": "projects/shop-ui/src/index.html",
            "browser": "projects/shop-ui/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/shop-ui/tsconfig.app.json",
            "assets": [
              "projects/shop-ui/src/favicon.ico",
              "projects/shop-ui/src/assets"
            ],
            "styles": ["projects/shop-ui/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "shop-ui:build:production"
            },
            "development": {
              "buildTarget": "shop-ui:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "shop-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/shop-ui/tsconfig.spec.json",
            "assets": [
              "projects/shop-ui/src/favicon.ico",
              "projects/shop-ui/src/assets"
            ],
            "styles": ["projects/shop-ui/src/styles.css"],
            "scripts": []
          }
        }
      }
    },
    "thinking-in-NG": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/thinking-in-ng",
      "sourceRoot": "projects/thinking-in-ng/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/thinking-in-ng",
            "index": "projects/thinking-in-ng/src/index.html",
            "browser": "projects/thinking-in-ng/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/thinking-in-ng/tsconfig.app.json",
            "assets": [
              "projects/thinking-in-ng/src/favicon.ico",
              "projects/thinking-in-ng/src/assets"
            ],
            "styles": [
              "projects/thinking-in-ng/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "thinking-in-NG:build:production"
            },
            "development": {
              "buildTarget": "thinking-in-NG:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "thinking-in-NG:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/thinking-in-ng/tsconfig.spec.json",
            "assets": [
              "projects/thinking-in-ng/src/favicon.ico",
              "projects/thinking-in-ng/src/assets"
            ],
            "styles": [
              "projects/thinking-in-ng/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
